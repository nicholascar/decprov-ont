###  http://promsns.org/def/do#had_answer
:had_answer rdf:type owl:ObjectProperty ;
            owl:inverseOf :was_answer_for ;
            rdfs:domain :Question ;
            rdfs:range :Answer ;
            rdfs:label "had answer"@en .


###  http://promsns.org/def/do#had_consequence
:had_consequence rdf:type owl:ObjectProperty ;
                 owl:inverseOf :was_consequence_of ;
                 rdfs:domain :Situation ;
                 rdfs:range :Situation ;
                 rdfs:label "had consequence"@en .


###  http://promsns.org/def/do#had_context
:had_context rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :occurs_in ;
             owl:inverseOf :was_context_of ;
             rdfs:range :Context ;
             rdfs:label "had context"@en .


###  http://promsns.org/def/do#had_criterion
:had_criterion rdf:type owl:ObjectProperty ;
               rdfs:label "had criterion"@en .


###  http://promsns.org/def/do#had_result
:had_result rdf:type owl:ObjectProperty ;
            owl:inverseOf :was_result_of ;
            rdfs:label "had result"@en .


###  http://promsns.org/def/do#had_solution
:had_solution rdf:type owl:ObjectProperty ;
              owl:inverseOf :was_solution_for ;
              rdfs:domain :Problem ;
              rdfs:label "had solution"@en .


###  http://promsns.org/def/do#includes
:includes rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :was_occurence_of ;
          owl:inverseOf :was_included_in ;
          rdfs:label "includes"@en .


###  http://promsns.org/def/do#indicates
:indicates rdf:type owl:ObjectProperty ;
